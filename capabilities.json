                            }
                        },
                        {
                            "bind": {
                                "to": "status"
                            }
                        }
                    ],
                    "dataReductionAlgorithm": {
                        "top": {
                            "count": 10000
                        }
                    }
                },
                "values": {
                    "group": {
                        "by": "Category",
                        "select": [
                            {
                                "bind": {
                                    "to": "values"
                                }
                            }
                        ],
                        "dataReductionAlgorithm": {
