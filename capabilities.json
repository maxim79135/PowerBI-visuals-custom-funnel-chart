{
    "dataRoles": [
        {
            "displayName": "Stage",
            "name": "stage",
            "kind": "Grouping"
        },
        {
            "displayName": "Status",
            "name": "status",
            "kind": "Grouping"
        },
        {
            "displayName": "Values",
            "name": "values",
            "kind": "Measure"
        }
    ],
    "objects": {
        "stage": {
            "displayName": "Stage",
            "displayNameKey": "stage",
            "properties": {}
        },
        "status": {
            "displayName": "Status",
            "displayNameKey": "status",
            "properties": {}
        },
        "values": {
            "displayName": "Values",
            "displayNameKey": "Values",
            "properties": {}
        }
    },
    "dataViewMappings": [
        {
            "conditions": [
                {
                    "stage": {
                        "min": 1,
                        "max": 1
                    },
                    "status": {
                        "max": 1
                    },
                    "values": {
                        "max": 1
                    }
                }
            ],
            "categorical": {
                "categories": {
                    "select": [
                        {
                            "for": {
                                "in": "stage"
                            }
                        },
                        {
                            "bind": {
                                "to": "status"
                            }
                        }
                    ],
                    "dataReductionAlgorithm": {
                        "top": {
                            "count": 10000
                        }
                    }
                },
                "values": {
                    "group": {
                        "by": "Category",
                        "select": [
                            {
                                "bind": {
                                    "to": "values"
                                }
                            }
                        ],
                        "dataReductionAlgorithm": {
                            "top": {}
                        }
                    }
                }
            }
        }
    ],
    "sorting": {
        "default": {}
    },
    "supportsLandingPage": true,
    "supportsEmptyDataView": true,
    "suppressDefaultTitle": true
}